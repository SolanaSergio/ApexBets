{
  "name": "project-apex",
  "description": "Advanced sports betting analytics platform with ML predictions and value betting detection",
  "version": "1.0.0",
  "private": true,
  "author": "Project Apex Team",
  "license": "MIT",
  "keywords": [
    "sports analytics",
    "betting",
    "machine learning",
    "predictions",
    "next.js"
  ],
  "homepage": "https://github.com/projectapex/apex",
  "repository": {
    "type": "git",
    "url": "https://github.com/projectapex/apex.git"
  },
  "packageManager": "pnpm@9.15.9",
  "engines": {
    "node": "18.x",
    "pnpm": "9.15.9"
  },
  "pnpm": {
    "overrides": {
      "@typescript-eslint/parser": "8.44.1",
      "@typescript-eslint/eslint-plugin": "8.44.1",
      "@typescript-eslint/utils": "8.44.1",
      "@typescript-eslint/type-utils": "8.44.1"
    }
  },
  "scripts": {
    "dev": "pnpm run build:server && node server.js",
    "dev:next": "next dev",
    "build": "next build",
    "build:server": "tsc -p tsconfig.server.json && powershell -Command \"Copy-Item startup.config.json dist/server/\"",
    "build:all": "pnpm run build:server && pnpm run build",
    "start": "pnpm run build:server && node server.js",
    "start:next": "next start",
    "lint": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:server": "tsc -p tsconfig.server.json --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "clean": "rm -rf .next out dist",
    "clean:server": "rm -rf dist/server",
    "setup": "node scripts/setup-environment.js",
    "setup:database": "node scripts/setup-database-simple.js",
    "populate:data": "node scripts/populate-real-data.js",
    "health": "curl http://localhost:3000/api/health",
    "health:detailed": "curl http://localhost:3000/api/health?detailed=true",
    "test": "jest --config jest.config.js",
    "test:unit": "jest --config jest.config.js --testPathPattern=unit",
    "test:integration": "jest --config jest.config.js --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage",
    "verify": "node tests/quick-verification.js",
    "verify:full": "node tests/comprehensive-tests.js",
    "mcp:start": "node supabase-mcp-server.js",
    "mcp:dev": "nodemon supabase-mcp-server.js",
    "mcp:test": "node test-mcp-server.js",
    "mcp:setup": "node scripts/setup/setup-mcp-database.js",
    "startup:status": "curl http://localhost:3000/api/startup?action=status",
    "startup:health": "curl http://localhost:3000/api/startup?action=health",
    "startup:restart": "curl -X GET http://localhost:3000/api/startup?action=restart",
    "startup:stop": "curl -X GET http://localhost:3000/api/startup?action=stop",
    "sync:data": "pnpm run build:server && node scripts/sync-sports-data.js",
    "sync:test": "pnpm run build:server && node scripts/sync-sports-data.js",
    "deploy:edge-function": "node scripts/deploy-edge-function.js",
    "test:system": "node scripts/test-complete-system.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.57.4",
    "@vercel/analytics": "latest",
    "@vercel/speed-insights": "^1.2.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "date-fns": "latest",
    "dotenv": "^17.2.2",
    "embla-carousel-react": "8.5.1",
    "geist": "^1.5.1",
    "input-otp": "1.4.1",
    "lucide-react": "^0.454.0",
    "next": "^15.5.3",
    "next-themes": "latest",
    "eslint-config-next": "^15.5.3",
    "@tailwindcss/postcss": "^4.1.13",
    "tailwindcss": "^4.1.13",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/node": "^22.18.6",
    "postcss": "^8.5.6",
    "eslint": "^9.36.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "react": "^18.3.1",
    "tw-animate-css": "1.3.8",
    "react-day-picker": "9.8.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.63.0",
    "react-resizable-panels": "^2.1.9",
    "recharts": "latest",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "vaul": "^0.9.9",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node-fetch": "^2.6.13",
    "@types/uuid": "^10.0.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "pm2": "^5.4.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4"
  }
}
